      },
      "source": [
        "#Encoding the ouput quantum states to a particular rhythmic beat\n",
        "rhythm_dic = {'000':4, '001':3, '100':1, '010':2, '011':1.5, '101':0.75, '111':0.25, '110':0.5}"
      ],
