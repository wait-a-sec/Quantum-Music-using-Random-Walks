      "source": [
        "#Encoding the ouput quantum states to a particular pitch\n",
        "#The numbers correspond to a MIDI frequency \n",
        "note_dic = {'000':60, '001':62, '100':64, '010':65, '011':67, '101':69, '111':71, '110':72}"
      ],
      "execution_count": null,
